<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>EducationPress - The Most Complete Education Solution</title>

    <!--[if lt IE 9]>
      <script src="js/html5shiv.min.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->

    <link rel="shortcut icon" href="/images/favicon.png" />

    <link href="/css/bootstrap.min.css" rel="stylesheet">

    <link rel="stylesheet" href="/css/font-awesome.min.css">

    <link rel="stylesheet" href="/css/linearicons-free.css">

    <link rel="stylesheet" href="/css/animate.css">

    <link rel="stylesheet" href="/vendor/flexslider/flexslider.css">

    <link rel="stylesheet" href="/vendor/owlcarousel/owl.carousel.css">

    <link rel="stylesheet" href="/vendor/fancybox/css/fancybox.css">
    <link rel="stylesheet" href="/css/custom.css">
    <link rel="stylesheet" href="/css/global.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/kindergarten.css">
</head>

<body>
    <div class="page-spacer clearfix">
        <div id="primary" class="content-area">
            <div class="container">
                <div class="row">
                    <main id="main" class="site-main col-xs-12">
                        <div class="col-xs-12 col-sm-10 register-form form clearfix">
                            <h3>Nộp bài tập</h3>
                            <form action="#" enctype="multipart/form-data" class="student-settings" method="post">
                                <div class="panel-group">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                Tiêu đề </h4>
                                        </div>
                                        <div class="panel-collapse collapsed" id="custom-collapse-1"
                                            data-toggle="collapse">
                                            <div class="panel-body">
                                                <%=exercise.title%> <br>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                Hướng dẫn</h4>
                                        </div>
                                        <div class="panel-collapse collapsed" id="custom-collapse-1"
                                            data-toggle="collapse">
                                            <div class="panel-body">
                                                <%=exercise.description%> <br>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                Hạn chót </h4>
                                        </div>
                                        <div class="panel-collapse collapsed" id="custom-collapse-1"
                                            data-toggle="collapse">
                                            <div class="panel-body">
                                                <input type="datetime-local" id="birthdaytime" name="deadline"
                                                    value="<%=exercise.deadline%>" disabled>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h4 class="panel-title">
                                                Nộp bài </h4>
                                        </div>
                                        <div class="panel-collapse collapsed" id="custom-collapse-1"
                                            data-toggle="collapse">
                                            <div class="panel-body">
                                                <input type="file" id="myFile" name="file">

                                                <textarea placeholder="Chú thích (nếu có)" name="message" id="message"
                                                    style="margin: 0px; width: 800x; height: 155px;"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <input name="submit" class="btn btn-default pull-right" value="Nộp bài" type="submit">
                                <a class="btn btn-default btn-medium pull-right"
                                    href="/classes/class-detail/<%=classDetail.classCode%>/courses"> Đóng </a>
                            </form>
                        </div>
                    </main>
                </div>
            </div>
        </div>
    </div>
</body>

<script>
    var expanded = false;

    function showCheckboxes() {
        var checkboxes = document.getElementById("checkboxes");
        if (!expanded) {
            checkboxes.style.display = "block";
            expanded = true;
        } else {
            checkboxes.style.display = "none";
            expanded = false;
        }
    }
</script>

<script language="javascript">

    document.getElementById("checkall").onclick = function () {
        // Lấy danh sách checkbox
        var checkboxes = document.getElementsByName("selected[]");
        if (this.checked) {
            // Lặp và thiết lập checked
            for (var i = 1; i < checkboxes.length; i++) {
                checkboxes[i].checked = true;
            }
        }
        if (!this.checked) {
            // Lặp và thiết lập unchecked
            for (var i = 1; i < checkboxes.length; i++) {
                checkboxes[i].checked = false;
            }
        }
    };

</script>

